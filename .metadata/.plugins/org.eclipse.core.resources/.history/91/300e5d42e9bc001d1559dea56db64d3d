package clases;

import java.awt.BorderLayout;
import java.awt.Dimension;

import javax.swing.ImageIcon;
import javax.swing.JFrame;

public class Ventana extends JFrame {

	public Ventana() {
    	//Declarar ventana y establecer tama√±o y color
        JFrame ventana = new JFrame("asd");
        ventana.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
        ventana.setLayout(new BorderLayout());
        ventana.setMinimumSize(new Dimension(600,400));
        ImageIcon favicon = new ImageIcon("C:\\Users\\EdmilsonDS\\asd\\ASD\\src\\favicon.png");
        ventana.setIconImage(favicon.getImage());
		
	}

}
